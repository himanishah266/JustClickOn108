<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD//EN"
"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">

<hibernate-configuration>
	<session-factory>
		<property name="hibernate.connection.driver_class"> com.mysql.jdbc.Driver</property>
		<property name="hibernate.connection.url">jdbc:mysql://localHost/justclickon108</property>
		<property name="hibernate.connection.username">root</property>
		<property name="hibernate.connection.password">root</property>
		<property name="hibernate.connection.pool_size">10</property>
		<property name="show_sql">true</property>
		<property name="dialect">org.hibernate.dialect.MySQLDialect</property>
		<property name="hibernate.hbm2ddl.auto">update</property>
		
		<mapping resource="VO/StateVO.hbm.xml"/> 
		<mapping resource="VO/CityVO.hbm..xml"/> 
		<mapping resource="VO/AreaVO.hbm.xml"/>
		<mapping resource="VO/AmbulanceVO.hbm.xml"></mapping>
		<mapping resource="VO/HospitalVO.hbm.xml"></mapping>
		<mapping resource="VO/StaffVO.hbm.xml"></mapping>
		<mapping resource="VO/RegisterVO.hbm.xml"></mapping>
		<mapping resource="VO/LoginVO.hbm.xml"></mapping>
		<mapping resource="VO/ComplaintVO.hbm.xml"></mapping>
		<mapping resource="VO/FeedbackVO.hbm.xml"></mapping>
			<mapping resource="VO/LocationVO.hbm.xml"></mapping>
					
					
		
		</session-factory>
</hibernate-configuration>